# 环境变量配置文件模板
# 请复制此文件为 .env 并填入实际的配置信息

# ==================== 数据库配置 ====================
# MySQL 主数据库 (reits库)
DB_HOST=127.0.0.1
DB_PORT=3306
DB_USER=root
DB_PASSWORD=your_password_here
DB_NAME=reits

# MySQL 公告数据库 (announcement库)
DB_ANNOUNCEMENT_HOST=127.0.0.1
DB_ANNOUNCEMENT_PORT=3306
DB_ANNOUNCEMENT_USER=root
DB_ANNOUNCEMENT_PASSWORD=your_password_here
DB_ANNOUNCEMENT_NAME=announcement

# ==================== 向量数据库配置 ====================
# Milvus 向量数据库
MILVUS_HOST=localhost
MILVUS_PORT=19530
MILVUS_USER=root
MILVUS_PASSWORD=your_password_here

# ==================== Elasticsearch 配置 ====================
ES_HOST=127.0.0.1
ES_PORT=9200
ES_USERNAME=elastic
ES_PASSWORD=your_password_here
ES_SCHEME=http

# ==================== AI模型API配置 ====================
# 智谱AI API密钥
ZHIPU_API_KEY=your_zhipu_api_key_here

# 阿里云API密钥
ALI_API_KEY=your_ali_api_key_here

# ==================== 文件路径配置 ====================
# PDF文件目录路径
PDF_DIR=./data/pdfs/

# 输出文件夹路径
OUTPUT_DIR=./data/output/

# Table Transformer模型路径
TABLE_TRANSFORMER_PATH=./models/table-transformer-detection

# 重排序模型路径（可选）
RERANK_MODEL_PATH=./models/bge-reranker-large